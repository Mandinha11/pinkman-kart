-- Select Join:

SELECT fornecedor.cnpj, karts.fornecedor_cnpj
FROM fornecedor
INNER JOIN karts
ON 
fornecedor.cnpj = karts.fornecedor_cnpj; 

SELECT fornecedor.cnpj, karts.fornecedor_cnpj
FROM fornecedor
LEFT JOIN karts
ON 
fornecedor.cnpj = karts.fornecedor_cnpj; 

SELECT fornecedor.cnpj, karts.fornecedor_cnpj
FROM fornecedor
RIGHT JOIN karts
ON 
fornecedor.cnpj = karts.fornecedor_cnpj; 
